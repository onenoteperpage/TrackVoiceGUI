<Window x:Class="TrackVoiceGUI.Windows.CredentialsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TrackVoiceGUI.Windows"
        mc:Ignorable="d"
        Title="CredentialsWindow" Height="450" Width="800">
    <StackPanel Orientation="Vertical">
        <DataGrid x:Name="myDataGrid" AutoGenerateColumns="True" CanUserAddRows="True"/>
        <Button x:Name="SaveDataGrid"
                Content="Save Changes"
                Width="140"
                Margin="10"
                Padding="3"
                Click="SaveDataGrid_Click"/>
    </StackPanel>
</Window>
